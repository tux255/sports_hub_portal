<% content_for(:add_item_button) { link_to 'Add Team', new_admin_team_path, class: 'btn btn-primary' } %>
<div class="row py-3">
  <h3>Teams</h3>
  <% @teams.each do |team| %>
    <div class="col-12">
      <%= team.name %>
      <%= team.category_id %>
      <%= image_tag team.team_logo, class: 'img-fuid',size:'200x200' unless team.team_logo.blank? %>
    </div>
  <% end %>
</div>
